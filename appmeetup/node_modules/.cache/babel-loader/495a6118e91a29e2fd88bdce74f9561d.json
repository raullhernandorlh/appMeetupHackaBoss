"><fa-icon :icon=\"['fas','times']\" size=\"2x\" /></button>\n            </div>\n       </div>\n    </div>\n</template>\n\n\n\n<script>\n\n// Imports for this View\nimport axios from 'axios'\nimport search from '@/components/Search.vue'\n\nexport default {\n    name:\"SearchByLocation\",\n    components:{\n        searchbycategory\n    },\n    data(){\n        return{\n            meetupsByCategories:[],\n            id:\"\",\n            date:\"\",\n            time:\"\",\n            duration:\"\",\n            location:\"\",\n            category:\"\",\n            meetup_principal_image:\"\",\n            meetup_second_image:\"\",\n            meetup_third_image:\"\",\n            meetup_price:\"\",\n            description:\"\",\n            meetupsList:\"false\",\n            seeModal:false\n        }\n    },\n\n    methods:{\n    \n    showMeetupDetail(dataMeetup){\n        this.id = dataMeetup.id;\n        this.date = dataMeetup.date\n        this.time = dataMeetup.time\n        this.duration = dataMeetup.duration\n        this.category = dataMeetup.category\n        this.meetup_principal_image= dataMeetup.meetup_principal_image\n        this.meetup_second_image= dataMeetup.meetup_second_image\n        this.meetup_third_image= dataMeetup.meetup_third_image\n        this.meetup_price= dataMeetup.meetup_price\n        this.meetup_description= dataMeetup.meetup_description\n\n        //Activamos el modal para que se vea\n        this.seeModal = true;\n    },\n\n    getMeetups(){\n        var self = this;\n        //Llama de axios a la api\n        axios.get(\"http://localhost:8888/meetups/\")\n        .then(function (response) {\n         console.log(response)\n        self.meetupsByCategories = response.data;\n        })\n        .catch(function (error) {\n        console.log(error);\n        })\n    },\n\n        },\n        created(){\n            this.getMeetups();\n        }\n    \n}\n</script>\n\n<style scoped>\n\n.modalBox{\n    display:flex;\n    flex-direction: column;\n    justify-content:center;\n    align-items: center;\n    border: 3px 0px solid black;\n    margin-bottom:5rem;\n}\n\n.text{\n    width:30%;\n    height:1.5rem;\n    margin-bottom:1rem;\n    border-radius:20rem;\n    text-align:center;\n   \n}\n\n\nspan{\n  margin-right:1rem\n}\n\n\n</style>"],"sourceRoot":"src/views/Search"}]}