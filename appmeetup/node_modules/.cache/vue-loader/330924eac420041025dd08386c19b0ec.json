lass=\"buttons\">\n                        <button @click=\"sendUserIndex(index)\">Delete</button>\n                        <button @click=\"sendDataUser(index)\">Detail</button>\n                    </div>\n                </div>\n            </li>\n        </ul>\n  </div>\n</template>\n\n<script>\nexport default {\n    name:\"AdministratorUsers\",\n    props:{\n        users:Array\n    },\n    methods:{\n        sendUserIndex(index){\n            let userIndex = this.users[index].id;\n            this.$emit('indice',userIndex)\n        },\n        sendDataUser(index){\n            let dataUser = this.users[index]\n            console.log(\"Estoy en el send data user\" + index)\n            this.$emit('data',dataUser)\n        }\n    }\n}\n</script>\n\n\n\n<style scoped>\n\nli{\n    list-style:none;\n}\n\n.users-list{\n    display:flex;\n    width:90%;\n    justify-content:space-between;\n    align-items: center;\n    flex-wrap: wrap;\n    margin-bottom: 5rem;\n}\n\n.users-data{\n    border:8px outset #00a896;\n    margin-left: 2rem;\n    padding:1rem;\n    margin-bottom:3rem;\n}\n\nbutton{\n    width: 7rem;\n    height:2rem;\n    margin:0.5rem;\n    border-radius:20rem;\n}\n\n.users{\n    margin-top:6rem;\n}\n\n\n\nbutton:hover{\n  background-color:#fff;\n  color:#00a896;\n  border:3px solid #00a896;\n\n\n}\nbutton{\n  background-color:#00a896;\n  color:#fff;\n  border:3px solid #fff ;\n  cursor:pointer;\n\n}\n\nimg{\n    width:20rem;\n    height:15rem;\n}\n\n\n</style>"]}]}