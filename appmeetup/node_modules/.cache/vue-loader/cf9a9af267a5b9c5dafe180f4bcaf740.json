  })\n          .catch(function (error) {\n          });\n\n          reservationsuccessmodal()\n    },\n\n    viewRating() {\n      const id = this.id\n      var self= this\n      axios.get(\"http://localhost:8888/ratings/avg/\" + id, {\n        })\n        .then(function (response) {\n\n           if (response.data.data.media === null) {\n            noRatingsForMeetup();\n          }\n\n          self.avg_rating = response.data.data.media;\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n\n  \n\n    getLogin(){\n      this.logged = isLoggedIn()\n    }\n  },\n  created(){\n    this.getLogin()\n  }\n\n\n};\n</script>\n\n<style scoped>\n\n.searchModalBox {\n  position: fixed;\n  top:0;\n  left:0;\n  right:0;\n  bottom:0;\n  width:100%;\n  background:rgb(0,0,0,0.5)\n}\n\n.modalBox{\n  display:flex;\n  justify-content:space-around;\n  background-color:#fff;\n  margin:5% auto;\n  padding: 3rem;\n  width:70%;\n  border:3px solid #00a896;\n  border-radius:12px;\n\n}\n\nspan{\n  margin:1rem;\n  font-size: 25px;\n}\n\n\n.images{\n  display:flex;\n  flex-direction: column;\n  width:30%;\n}\n\n\n\n.more-information{\n  display:flex;\n  width:60%;\n  align-items:center;\n  flex-direction: column;\n  margin-top:0.5rem;\n  margin-bottom:1rem;\n}\n\nimg{\n  width: 18rem;\n  height: 10rem;\n  border:4px outset #00a896;\n  margin-right:2rem;\n}\n\nbutton {\n  border-radius:20px; \n  width:8rem;\n  height:2.5rem;\n  margin-right:0.5rem;\n  color:#fff;\n  text-decoration: none;\n  border:3px solid white;\n  background-color: #00a896;\n  margin-bottom:1rem;\n  margin-top:2rem;\n\n\n}\n\nbutton a {\n  color:#fff;\n}\n\n.modalbox-buttons{\n  margin-top:1.5rem;\n}\nbutton:hover{\n  border:3px solid #00a896;\n  background-color: #fff;\n  color:#00a896;\n  font-weight: bold;\n  cursor:pointer;\n}\n\n.text{\n  width:50%;\n  border-radius:20rem;\n  height:1.5rem;\n  border:3px solid #00a896;  \n  text-align:center;\n  font-size:17px;\n}\n\n